// Editor config, es7 + react, eslint, gitlens, material icon, origamid next, prettier, wakatime.
{
  "editor.fontFamily": "IBM Plex Mono",
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "editor.lineHeight": 24,
  "editor.letterSpacing": 0,
  "files.autoSave": "afterDelay",
  "editor.suggest.showCustomcolors": false,
  "explorer.confirmDelete": false,
  "workbench.panel.defaultLocation": "right",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "editor.wordWrap": "on",
  "security.workspace.trust.untrustedFiles": "open",
  "breadcrumbs.filePath": "off",
  "outline.icons": false,
  // "editor.minimap.enabled": false,
  // "workbench.tree.indent": 20,
  "terminal.integrated.fontSize": 16,
  "files.associations": {
    "*.js": "javascript",
    "*.jsx": "javascriptreact",
    "*.ts": "typescript",
    "*.tsx": "typescriptreact"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "workbench.activityBar.visible": true,
  "explorer.confirmDragAndDrop": false,
  "diffEditor.renderSideBySide": true,
  "editor.formatOnSave": true,
  "html.autoClosingTags": false,
  // "editor.colorDecorators": false,
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "prettier.singleQuote": true,
  "editor.minimap.renderCharacters": false,
  "breadcrumbs.enabled": false,
  "editor.renderWhitespace": "selection",
  "workbench.statusBar.visible": true,
  "prettier.trailingComma": "all",
  "explorer.sortOrder": "default",
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "window.newWindowDimensions": "inherit",
  "html.format.wrapAttributes": "auto",
  "html.format.wrapLineLength": 0,
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Origamid Next",
  "workbench.iconTheme": "material-icon-theme",
  "git.enableSmartCommit": true,
  "window.zoomLevel": 1,
  "files.exclude": {
    "public": true,
    "README.md": true,
    ".gitignore": true,
    "package.json": true,
    "package-lock.json": true,
    "**/node_modules": true,
    ".next": true,
    ".eslintrc.json": true,
    "next-env.d.ts": true,
    "next.config.js": true,
    "tsconfig.json": true,
    "yarn.lock": true,
    ".github": true,
    ".husky": true,
    ".storybook": true,
    ".vscode": true,
    "docs": true,
    "generators": true,
    ".editorconfig": true,
    ".env": true,
    ".env.example": true,
    ".eslintignore": true,
    ".prettierignore": true,
    "commitlint.config.js": true,
    ".prettierrc.json": true,
    "tsconfig.paths.json": true,
    "test.js": true,
    "craco.config.js": true,
    "cypress.json": true,
    ".gitlab-ci.yml": true,
    ".eslintrc.js": true,
    "debug.log": true,
    "format.json": true,
    "jsconfig.json": true,
    ".htaccess": true,
    "Procfile": true,
    ".lintstagedrc.json": true,
    "react-leap": true,
    "index.html": true,
    "bash.exe.stackdump": true,
    "tsconfig.node.json": true,
    "vite.config.ts": true,
    // "-------------": true,
    // "DENTRO DE SRC": true,
    // "-------------": true,
    // "src/assets": true,
    // "src/contexts": true,
    // "src/helpers": true,
    // "src/styles": true,
    // "src/serviceWorker.js": true,
    // "src/index.jsx": true,
    // "src/App.jsx": true,
    // "src/setupTests.js": true,
    // "src/variables": true,
    // "src/hooks": true,
    // "src/api": true,
    // "src/setupTests.ts": true,
    // "src/reportWebVitals.ts": true,
    // "src/react-app-env.d.ts": true,
    // "src/utils": true,
    // "src/test": true,
    // "src/types": true,
    // "src/store": true,
    // "src/errors": true,
    // "src/context": true,
    // "src/config": true,
    // "src/lib": true,
    // "src/index.tsx": true,
    // "src/App.tsx": true,
    // "src/components": true,
    // "src/routes": true,
    // "src/modules": true,
  },
  "git.confirmSync": false
}
